{
  "name": "@krad/manson",
  "version": "0.0.0",
  "description": "a simple logger used to help debug player & media parsing issues",
  "main": "index.js",
  "scripts": {
    "test": "babel-tape-runner tests/**/*.js | tap-spec",
    "autotest": "tape-watch -c -r babel-register tests/**/*-tests.js | tap-spec",
    "docs": "jsdoc src/* -d docs",
    "build": "babel src --presets babel-preset-es2015 --out-dir distribution"
  },
  "author": "krad.tv <iam@krad.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^8.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "tape-watch": "^2.3.0"
  }
}
